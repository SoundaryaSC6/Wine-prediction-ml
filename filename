    "Model": "dt_pred1",
    'R2': [r2_score(y_test1, dt_pred1)],
    'RMSE': [mean_squared_error(y_test1, dt_pred1, squared=False)],
    'MAE': [mean_absolute_error(y_test1, dt_pred1)],
    'MSE': [mean_squared_error(y_test1, dt_pred1)]
})

