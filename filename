sns.heatmap(cm_svc, annot=True, fmt='d', cmap='Greens', xticklabels=svc.classes_, yticklabels=svc.classes_)
plt.xlabel('Predicted')
plt.ylabel('Actual')
plt.title('Confusion Matrix')
plt.show()
