    "Model": "rf_model1",
    'R2': [r2_score(y_test1, rf_predictions1)],
    'RMSE': [mean_squared_error(y_test1, rf_predictions1, squared=False)],
    'MAE': [mean_absolute_error(y_test1, rf_predictions1)],
