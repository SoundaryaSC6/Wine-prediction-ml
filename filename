print(scaled_probs)

def min_max_scaling(data):
    min_val = np.min(data)
    max_val = np.max(data)
    epsilon = 1e-10  # Small constant to prevent division by zero
    scaled_data = (data - min_val) / (max_val - min_val + epsilon)
    return scaled_data
