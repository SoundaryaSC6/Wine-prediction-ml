    'Residual sugar': [2.2, 2.4, 2.6, 2.8, 3, 2, 2.2, 2.4, 2.6, 2.8, 3, 2.2, 2.4, 2.6],
    'Chlorides': [0.021, 0.025, 0.029, 0.033, 0.037, 0.019, 0.023, 0.027, 0.031, 0.035, 0.039, 0.021, 0.025, 0.029],
