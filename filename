plt.ylabel("Count")
plt.legend(['Non-Churn', 'Churn'])

# Add annotations to the bars
for p in ax.patches:
    ax.annotate(f'{p.get_height()}', (p.get_x() + p.get_width() / 2., p.get_height()), ha='center', va='bottom')

