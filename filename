    ['100001', '3', 1, 50.00],
    ['100002', '1', 2, 100.00],
    ['100003', '4', 3, 150.00],
    ['100004', '1', 4, 200.00],
    ['100005', '0', 5, 250.00],
    ['100006', '3', 1, 50.00],
    ['100007', '1', 2, 100.00],
    ['100008', '2', 3, 150.00],
    ['100009', '2', 4, 200.00],
    ['100010', '1', 5, 250.00],
]

yn_pred = tree_cv.predict(new_data)
