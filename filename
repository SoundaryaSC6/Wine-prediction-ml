        try:
            value, unit = dosage.split()
            dosage_in_mg = float(value) * unit_conversions.get(unit, 1)
            return dosage_in_mg
    else:
        print(f"Invalid dosage format: {dosage}")
        return None 

Dosage_Recommendation = data['Dosage Recommendation'].apply(convert_to_mg)
