    "Model": "rf_model2",
    'R2': [r2_score(y_test2, rf_predictions2)],
    'RMSE': [mean_squared_error(y_test2, rf_predictions2, squared=False)],
    'MAE': [mean_absolute_error(y_test2, rf_predictions2)],
    'MSE': [mean_squared_error(y_test2, rf_predictions2)]
})
