    max_depth=5,
    learning_rate=0.1,
    n_estimators=100,
    subsample=0.8,
    colsample_bytree=0.8,
    random_state=42
)

# Model 3: XGBoost with another set of parameters
model_3 = XGBClassifier(
    max_depth=3,
    learning_rate=0.2,
    n_estimators=200,
    subsample=0.9,
    colsample_bytree=0.9,
    random_state=42
)

