clf = DecisionTreeClassifier()
clf = tree_cv.fit(Xr_train,yr_train)
yr_pred = tree_cv.predict(Xr_test)
print(yr_pred)

print("Accuracy:",metrics.accuracy_score(yr_test, yr_pred))


