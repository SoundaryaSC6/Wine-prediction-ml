q3 = dataset['Probability*'].quantile(0.75)
iqr = q3 - q1
lower_threshold = q1 - 1.5 * iqr
upper_threshold = q3 + 1.5 * iqr
dataset_NO = dataset[(dataset['Probability*'] >= lower_threshold) & (dataset['Probability*'] <= upper_threshold)]
