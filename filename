rmse = mean_squared_error(y_test2, svr_pred2, squared=False)
mae = np.sqrt(mean_absolute_error(y_test2, svr_pred2))
mse = mean_squared_error(y_test2, svr_pred2)
results_svr_regressor2 = pd.DataFrame({
    "Model": "svr_regressor2",
