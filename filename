    'citric acid': [0.10],
    'residual sugar': [2.5],
    'chlorides': [0.075],
    'free sulfur dioxide': [15.0],
    'total sulfur dioxide': [30.0],
    'density': [0.998],
    'pH': [3.4],
    'sulphates': [0.55],
    'alcohol': [9.5],  # Set alcohol to 9.5
    'quality': [0]  # Add a placeholder for quality (you can replace 0 with any value)
}

