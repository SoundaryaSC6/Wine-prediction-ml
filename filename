# Scaling and Normalization
from sklearn.preprocessing import StandardScaler
import numpy as np

probs = np.array(dataset['Probability*'])
probs_2d = probs.reshape(-1, 1)
scaler = StandardScaler()
scaled_probs = scaler.fit_transform(probs_2d)
