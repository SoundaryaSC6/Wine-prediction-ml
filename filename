print("Predicted antagonism, synergism, and agonism of the 10 drugs:")
for i in range(len(y_pred_10)):
    if y_pred_10[i] == 1:
        print("Antagonism")
    elif y_pred_10[i] == 2:
        print("Synergism")
    elif y_pred_10[i] == 3:
